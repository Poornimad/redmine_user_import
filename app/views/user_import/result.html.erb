<% html_title l(:user_import) -%>
<% content_for :header_tags do %>
    <%= stylesheet_link_tag 'imoprter', :plugin => 'redmine_user_import' %>
<% end %>

<h2><%= l(:label_import_result) %></h2>
<p><%= l(:label_result_notice, @handle_count, @handle_count - @failed_count) %></p>

<hr/>

<% if @failed_count > 0 %>
<%= l(:label_result_failed, @failed_count) %>
<table class="list">
    <thead><tr>
        <th>#</th>
        <% @headers.each do |column| %>
          <th><%= column %></th>
        <% end %>
    </tr></thead>
    <tbody>
    <% @failed_rows.each do |id, row| -%>
    <tr class="<%= cycle("odd", "even") %>">
        <td><%= id %></td>
        <% row.each do |column| %><%= content_tag 'td', column[1] %><% end %>
    </tr>
    <% end %>
    </tbody>
</table>
<% end %>
